                  </variable>
                </localVars>
              </interface>
              <body>
                <ST>
                  <xhtml xmlns="http://www.w3.org/1999/xhtml">

FOR counter:= 2 TO 6 BY 2 DO
	CASE counter OF
	1, 3:  
		BOOL1 := TRUE;
	2:     
		BOOL2 := FALSE;
	4:     
		BOOL2 := NOT BOOL2;
	ELSE
		BOOL1 := NOT BOOL1;
	END_CASE;
END_FOR;

</xhtml>
                </ST>
              </body>
